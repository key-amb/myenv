__load_files=(
  "$HOME/.travis/travis.sh"
)

case "$SHELL" in
  */bash )
    . "$MYENV_ROOT/common/shrc.d/load_vendor_apps.bashrc";;
  */zsh )
    . "$MYENV_ROOT/common/shrc.d/load_vendor_apps.zshrc";;
  * )
    ;;
esac

for __src in "${__load_files[@]}"; do
  [[ -f "${__src}" ]] && . "${__src}"
done

unset __load_files __src
